<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanyuan.mapper.BmiNormalConfigMapper">
    <cache type="org.mybatis.caches.memcached.LoggingMemcachedCache" />
	<!--mybatis ehcache缓存配置 -->
	<!-- 以下两个<cache>标签二选一,第一个可以输出日志,第二个不输出日志 <cache type="org.mybatis.caches.ehcache.LoggingEhcache" 
		/> -->
	<!-- <cache type="org.mybatis.caches.ehcache.EhcacheCache"/> -->
	<!-- 以下与实体类的中字段一致 -->


	<select id="findSexNormalConfig" resultType="com.lanyuan.entity.BmiNormalConfigFormMap">
SELECT
  bnc.id,
  bnc.sex,
  bnc.min_bmi,
  bnc.max_bmi
FROM bmi_normal_config bnc
WHERE bnc.sex = ${sex}
ORDER BY bnc.id DESC LIMIT 1
	</select>



    <select id="findPage" resultType="com.lanyuan.entity.BmiNormalConfigFormMap">
        SELECT
        bnc.id,
        bnc.sex,
        bnc.min_bmi,
        bnc.max_bmi
        FROM bmi_normal_config bnc
        ORDER BY bnc.id ASC
    </select>


</mapper>